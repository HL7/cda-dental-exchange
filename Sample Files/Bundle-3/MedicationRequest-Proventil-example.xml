<?xml version="1.0" encoding="UTF-8"?>

<MedicationRequest xmlns="http://hl7.org/fhir">
<!-- example using medication codes -->
  <id value="dental-medreq-2"/>
  <meta>
    <profile value="http://hl7.org/fhir/us/core/StructureDefinition/us-core-medicationrequest"/>
  </meta>
  
  <status value="active"/>
  <intent value="plan"/>
  <medicationCodeableConcept>
    <coding>
       <system value="http://www.nlm.nih.gov/research/umls/rxnorm"/>
      <code value="746763"/>
      <display value="Proventil HFA 90 MCG/ACTUAT Meter Dose Inhaler, 200 ACTUAT"/>
    </coding>
    <text value="Proventil HFA 90 MCG/ACTUAT Meter Dose Inhaler, 200 ACTUAT"/>
  </medicationCodeableConcept>
  <subject>
    <reference value="Patient/example-dental"/>
    <display value="Patient A"/>
  </subject>
  <authoredOn value="2010-12-06"/>
  <requester>
    <reference value="Practitioner/practitioner-1"/>
    <display value="Dr. John M, MD"/>
  </requester>
<!--   either a code or a reference to the medication being prescribed. As described earlier, this could be contained, separately        within a bundle or simply a reference to a remote server.  -->
<!-- 	The following elements are not mandatory for the uscorenaut Profile  -->
  <dosageInstruction>
    <text value="2 puffs every 2-4 hours as needed"/>
    <timing>
      <repeat>
        <boundsPeriod>
          <start value="2010-12-06"/>
        </boundsPeriod>
      </repeat>
    </timing>
  </dosageInstruction>
  <dispenseRequest>
    <numberOfRepeatsAllowed value="12"/>
    <quantity>
      <value value="480"/>
      <unit value="mL"/>
      <system value="http://unitsofmeasure.org"/>
      <code value="mL"/>
    </quantity>
    <expectedSupplyDuration>
      <value value="30"/>
      <unit value="days"/>
      <system value="http://unitsofmeasure.org"/>
      <code value="d"/>
    </expectedSupplyDuration>
  </dispenseRequest>
</MedicationRequest>